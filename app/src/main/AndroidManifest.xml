<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.getlose.mytemplate">
    <!-- 開啓網路權限 -->
    <uses-permission android:name="android.permission.INTERNET" /> <!-- 讀取聯絡人權限 -->
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyTemplate"
        android:usesCleartextTraffic="true">
        <activity android:name=".ComponentActivity"></activity>
        <activity android:name=".ApiActivity" />
        <activity android:name=".UrlConnectActivity" />
        <activity android:name=".RoomActivity" />
        <activity android:name=".ContentProviderActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".Service.CacheService" />
        <service android:name=".Service.CacheIntentService" />
    </application>

</manifest>